{
    "algo_name": "iql",
    "experiment": {
        "name": "geometry_cuboid-0.6mm-iql-test_100_abs_v1",
        "validate": false,
        "logging": {
            "terminal_output_to_txt": true,
            "log_tb": true,
            "log_wandb": true,
            "wandb_proj_name": "furniture-bench"
        },
        "save": {
            "enabled": true,
            "every_n_seconds": null,
